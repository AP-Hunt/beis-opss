
<% content_for :head do %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= csrf_meta_tags %>
<% end %>
<% content_for :homepage_url, "/" %>
<% content_for :logo_link_title, "Go to the homepage" %>
<% content_for :header_class, "with-proposition" %>
<% content_for :proposition_header do %>
    <%= render "layouts/header" %>
<% end %>
<% content_for :content do %>
    <div class="govuk-width-container">
        <%= render "layouts/alpha_tag" %>
        <%= render "layouts/notice" %>
        <main class="govuk-main-wrapper " id="main-content" role="main">
            <%= yield %>
        </main>
    </div>
<% end %>
<%= render file: "layouts/govuk_template" %>
<script>
  window.GOVUKFrontend.initAll()
</script>
